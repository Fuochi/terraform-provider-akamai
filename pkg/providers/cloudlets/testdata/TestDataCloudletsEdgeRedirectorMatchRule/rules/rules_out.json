[
  {
    "name": "rul3",
    "type": "erMatchRule",
    "matches": [
      {
        "matchType": "hostname",
        "matchValue": "3333.dom",
        "matchOperator": "equals",
        "caseSensitive": true,
        "negate": false
      },
      {
        "matchType": "cookie",
        "matchValue": "cookie=cookievalue",
        "matchOperator": "equals",
        "caseSensitive": false,
        "negate": false
      },
      {
        "matchType": "extension",
        "matchValue": "txt",
        "matchOperator": "equals",
        "caseSensitive": false,
        "negate": false
      }
    ],
    "useRelativeUrl": "copy_scheme_hostname",
    "statusCode": 307,
    "redirectURL": "/abc/sss",
    "useIncomingQueryString": false,
    "useIncomingSchemeAndHost": true
  },
  {
    "name": "rule 2",
    "type": "erMatchRule",
    "useRelativeUrl": "none",
    "statusCode": 301,
    "redirectURL": "sss.com",
    "matchURL": "ddd.aaa",
    "useIncomingQueryString": true,
    "useIncomingSchemeAndHost": false
  },
  {
    "name": "r1",
    "type": "erMatchRule",
    "useRelativeUrl": "copy_scheme_hostname",
    "statusCode": 301,
    "redirectURL": "/ddd",
    "matchURL": "abc.com",
    "useIncomingQueryString": false,
    "useIncomingSchemeAndHost": true
  }
]